{
    "name": "aedart/athenaeum-acl",
    "description": "ACL package for Laravel, with roles and permissions (grouped)",
    "keywords": [
        "Athenaeum",
        "Acl",
        "Access Controll List",
        "Roles",
        "Permissions",
        "Authorization"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^8.3",
        "aedart/athenaeum-contracts": "^9.4",
        "aedart/athenaeum-database": "^9.4",
        "aedart/athenaeum-support": "^9.4",
        "aedart/athenaeum-utils": "^9.4",
        "aedart/athenaeum-validation": "^9.4",
        "illuminate/auth": "^v12.9.2"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Acl\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
