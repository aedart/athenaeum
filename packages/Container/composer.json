{
    "name": "aedart/athenaeum-container",
    "description": "Slightly adapted version of Laravel's Service Container",
    "keywords": [
        "Athenaeum",
        "IoC",
        "Container",
        "Service Container"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "aedart/athenaeum-contracts": "^7.10.1",
        "aedart/athenaeum-support": "^7.10.1",
        "illuminate/container": "^v10.3.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Container\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
