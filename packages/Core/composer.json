{
    "name": "aedart/athenaeum-core",
    "description": "A custom Laravel Application implementation, intended to be testing, tinkering or developing non-essential custom applications.",
    "keywords": [
        "Athenaeum",
        "Core",
        "Application"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^8.4",
        "aedart/athenaeum-config": "^9.26",
        "aedart/athenaeum-console": "^9.26",
        "aedart/athenaeum-container": "^9.26",
        "aedart/athenaeum-contracts": "^9.26",
        "aedart/athenaeum-dto": "^9.26",
        "aedart/athenaeum-events": "^9.26",
        "aedart/athenaeum-maintenance-modes": "^9.26",
        "aedart/athenaeum-service": "^9.26",
        "aedart/athenaeum-support": "^9.26",
        "aedart/athenaeum-utils": "^9.26",
        "illuminate/cache": "^v12.53.0",
        "illuminate/filesystem": "^v12.53.0",
        "psr/log": "^3.0.2",
        "symfony/console": "^v7.4.6",
        "vlucas/phpdotenv": "^5.6.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^v7.4.6"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Core\\": "src"
        },
        "files": [
            "helpers/paths.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
