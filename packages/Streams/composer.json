{
    "name": "aedart/athenaeum-streams",
    "description": "Stream utilities",
    "keywords": [
        "Athenaeum",
        "Utilities",
        "stream",
        "resource"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "aedart/athenaeum-contracts": "^8.1",
        "aedart/athenaeum-mime-types": "^8.1",
        "aedart/athenaeum-support": "^8.1",
        "psr/http-message": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Streams\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
