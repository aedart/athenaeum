{
    "name": "aedart/athenaeum-utils",
    "description": "Offers a few utility components that are used by the Athenaeum packages",
    "keywords": [
        "Athenaeum",
        "Utilities",
        "Array",
        "Json",
        "Math",
        "Function",
        "Version"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "composer-runtime-api": "^2.0.0",
        "aedart/athenaeum-contracts": "^7.16",
        "illuminate/support": "^v10.13.0",
        "mockery/mockery": "1.5.*"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Utils\\": "src"
        }
    },
    "bin": [
        "bin/build-version"
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
