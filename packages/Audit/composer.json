{
    "name": "aedart/athenaeum-audit",
    "description": "Audit trail package for Laravel Eloquent",
    "keywords": [
        "Athenaeum",
        "Audit Trail",
        "Audit Log",
        "History",
        "Eloquent"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "aedart/athenaeum-contracts": "^5.22.4",
        "aedart/athenaeum-database": "^5.22.4",
        "aedart/athenaeum-support": "^5.22.4",
        "aedart/athenaeum-utils": "^5.22.4"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Audit\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "5.23.x-dev"
        }
    }
}
