{
    "name": "aedart/athenaeum-audit",
    "description": "Audit trail package for Laravel Eloquent",
    "keywords": [
        "Athenaeum",
        "Audit Trail",
        "Audit Log",
        "History",
        "Eloquent"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "aedart/athenaeum-contracts": "^7.16",
        "aedart/athenaeum-database": "^7.16",
        "aedart/athenaeum-support": "^7.16",
        "aedart/athenaeum-utils": "^7.16"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Audit\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
