{
    "name": "aedart/athenaeum-auth",
    "description": "Authentication related utilities for Laravel",
    "keywords": [
        "Athenaeum",
        "Authentication"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.22",
        "aedart/athenaeum-contracts": "^7.24",
        "aedart/athenaeum-support": "^7.24"
    },
    "require-dev": {
        "laravel/fortify": "^1.18.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Auth\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
