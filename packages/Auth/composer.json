{
    "name": "aedart/athenaeum-auth",
    "description": "Authentication related utilities for Laravel",
    "keywords": [
        "Athenaeum",
        "Authentication"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^8.3",
        "aedart/athenaeum-contracts": "^9.14",
        "aedart/athenaeum-support": "^9.14",
        "illuminate/validation": "^v12.32.5",
        "laravel/fortify": "^1.31.0",
        "shrikeh/teapot": "^3.0.0"
    },
    "require-dev": {
        "laravel/fortify": "^1.31.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Auth\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
