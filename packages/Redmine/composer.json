{
    "name": "aedart/athenaeum-redmine",
    "description": "Redmine API Client for Laravel",
    "keywords": [
        "Athenaeum",
        "Redmine",
        "API"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "aedart/athenaeum-dto": "^7.17",
        "aedart/athenaeum-http-clients": "^7.17",
        "aedart/athenaeum-pagination": "^7.17"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Redmine\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
