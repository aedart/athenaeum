{
    "name": "aedart/athenaeum-config",
    "description": "A loader that is able to load various types of configuration files",
    "keywords": [
        "Athenaeum",
        "Configuration",
        "Loader",
        "ini",
        "Json",
        "PHP Array",
        "yaml",
        "toml",
        "neon"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^8.3",
        "aedart/athenaeum-contracts": "^8.22",
        "aedart/athenaeum-support": "^8.22",
        "aedart/athenaeum-utils": "^8.22",
        "illuminate/config": "^v11.42.1",
        "illuminate/filesystem": "^v11.42.1"
    },
    "require-dev": {
        "nette/neon": "^3.4.4",
        "symfony/yaml": "^v7.2.3",
        "devium/toml": "^1.0.5"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Config\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
