{
    "name": "aedart/athenaeum-support",
    "description": "Aware-of Helpers for Laravel components and other utilities",
    "keywords": [
        "Athenaeum",
        "Helpers",
        "Aware Of",
        "Properties",
        "Utils"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "aedart/athenaeum-contracts": "^7.21",
        "aedart/athenaeum-utils": "^7.21",
        "illuminate/config": "^v10.19.0",
        "illuminate/support": "^v10.19.0"
    },
    "require-dev": {
        "predis/predis": "^2.2.1",
        "symfony/console": "^v6.3.2",
        "twig/twig": "^3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Support\\": "src"
        },
        "files": [
            "helpers/properties.php"
        ]
    },
    "bin": [
        "aware-of"
    ],
    "suggest": {
        "twig/twig": "Required for using aware-of generator"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
