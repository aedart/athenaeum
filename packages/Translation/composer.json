{
    "name": "aedart/athenaeum-translation",
    "description": "Translation utilities for Laravel",
    "keywords": [
        "Athenaeum",
        "Translation",
        "Exporters"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "aedart/athenaeum-support": "^8.3",
        "illuminate/translation": "^v11.4.0",
        "symfony/finder": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Translation\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
