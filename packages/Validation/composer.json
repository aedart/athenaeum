{
    "name": "aedart/athenaeum-validation",
    "description": "Validation rules and utilities for Laravel",
    "keywords": [
        "Athenaeum",
        "Validation"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^8.2",
        "aedart/athenaeum-contracts": "^8.16",
        "aedart/athenaeum-support": "^8.16",
        "aedart/athenaeum-utils": "^8.16",
        "illuminate/validation": "^v11.33.2"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Validation\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
