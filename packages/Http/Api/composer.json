{
    "name": "aedart/athenaeum-http-api",
    "description": "Json based API utilities",
    "keywords": [
        "Athenaeum",
        "Http",
        "Json",
        "API"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^8.2",
        "aedart/athenaeum-contracts": "^8.21",
        "aedart/athenaeum-etags": "^8.21",
        "aedart/athenaeum-support": "^8.21",
        "aedart/athenaeum-validation": "^8.21",
        "illuminate/http": "^v11.42.1",
        "shrikeh/teapot": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Http\\Api\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "suggest": {
        "laravel/framework": "Required if you want to make use of API Request abstractions"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
