{
    "name": "aedart/athenaeum-http-api",
    "description": "Json based API utilities",
    "keywords": ["Athenaeum", "Http", "Json", "API"],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "aedart/athenaeum-contracts": "^6.6",
        "aedart/athenaeum-support": "^6.6",
        "aedart/athenaeum-validation": "^6.6",
        "illuminate/http": "^v9.30.1",
        "shrikeh/teapot": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Http\\Api\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
