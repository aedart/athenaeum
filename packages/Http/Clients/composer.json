{
    "name": "aedart/athenaeum-http-clients",
    "description": "Http Client wrapper along with a Manager that is able to handle multiple client instances (profiles)",
    "keywords": [
        "Athenaeum",
        "Http Clients",
        "Psr-7",
        "Manager",
        "Profiles"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "aedart/athenaeum-container": "^7.21",
        "aedart/athenaeum-contracts": "^7.21",
        "aedart/athenaeum-http-cookies": "^7.21",
        "aedart/athenaeum-http-messages": "^7.21",
        "aedart/athenaeum-support": "^7.21",
        "guzzlehttp/guzzle": "^7.7.0",
        "illuminate/pipeline": "^v10.19.0",
        "psr/http-client": "^1.0.2",
        "psr/http-factory": "^1.0.2",
        "psr/http-message": "^1.1.0",
        "shrikeh/teapot": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Http\\Clients\\": "src"
        }
    },
    "suggest": {
        "symfony/var-dumper": "Required for using debugging utilities",
        "illuminate/log": "Required for logging requests / response"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
