        "aedart/athenaeum-contracts": "^7.21"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Http\\Cookies\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
