{
    "name": "aedart/athenaeum-testing",
    "description": "Testing utilities built on top of Orchestra Testbench and Codeception",
    "keywords": [
        "Athenaeum",
        "Testing"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^8.2",
        "aedart/athenaeum-container": "^8.15",
        "aedart/athenaeum-contracts": "^8.15",
        "codeception/codeception": "^5.1.2",
        "codeception/module-asserts": "^3.0.0",
        "fakerphp/faker": "^v1.24.0",
        "illuminate/testing": "^v11.33.2",
        "mockery/mockery": "1.6.*",
        "orchestra/testbench": "^v9.6.1",
        "orchestra/testbench-dusk": "^v9.9.1"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Testing\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
