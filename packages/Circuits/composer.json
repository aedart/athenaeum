{
    "name": "aedart/athenaeum-circuits",
    "description": "Circuit Breaker for Laravel",
    "keywords": [
        "Athenaeum",
        "Circuit Breaker"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "illuminate/cache": "^8.0",
        "illuminate/events": "^8.0",
        "aedart/athenaeum-contracts": "^4.3",
        "aedart/athenaeum-support": "^4.3",
        "aedart/athenaeum-utils": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Circuits\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.3.x-dev"
        }
    }
}
