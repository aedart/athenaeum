{
    "name": "aedart/athenaeum-circuits",
    "description": "Circuit Breaker for Laravel",
    "keywords": [
        "Athenaeum",
        "Circuit Breaker"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "aedart/athenaeum-contracts": "^8.7",
        "aedart/athenaeum-support": "^8.7",
        "aedart/athenaeum-utils": "^8.7",
        "illuminate/cache": "^v11.13.0",
        "illuminate/events": "^v11.13.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Circuits\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
