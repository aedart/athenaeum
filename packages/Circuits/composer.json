{
    "name": "aedart/athenaeum-circuits",
    "description": "Circuit Breaker for Laravel",
    "keywords": [
        "Athenaeum",
        "Circuit Breaker"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "aedart/athenaeum-contracts": "^7.17",
        "aedart/athenaeum-support": "^7.17",
        "aedart/athenaeum-utils": "^7.17",
        "illuminate/cache": "^v10.13.5",
        "illuminate/events": "^v10.13.5"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Circuits\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
