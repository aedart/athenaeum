{
    "name": "aedart/athenaeum-events",
    "description": "Offers a way to register event listeners and subscribers via configuration",
    "keywords": [
        "Athenaeum",
        "Events",
        "Listeners",
        "Subscribers"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "aedart/athenaeum-support": "^8.8",
        "illuminate/events": "^v11.20.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Events\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
