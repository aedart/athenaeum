{
    "name": "aedart/athenaeum-console",
    "description": "Offers a way to register commands and schedules via configuration",
    "keywords": [
        "Athenaeum",
        "Console",
        "Command",
        "Schedule"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "aedart/athenaeum-contracts": "^7.15",
        "aedart/athenaeum-support": "^7.15",
        "dragonmantank/cron-expression": "^3.3.2",
        "illuminate/cache": "^v10.13.0",
        "illuminate/console": "^v10.13.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Console\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
