{
    "name": "aedart/athenaeum-filters",
    "description": "Search filter utilities, based on http query parameters",
    "keywords": [
        "Athenaeum",
        "Search",
        "Filters",
        "Http",
        "API"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "aedart/athenaeum-contracts": "^7.21",
        "aedart/athenaeum-database": "^7.21",
        "aedart/athenaeum-support": "^7.21",
        "aedart/athenaeum-utils": "^7.21",
        "aedart/athenaeum-validation": "^7.21",
        "illuminate/http": "^v10.19.0",
        "illuminate/pipeline": "^v10.19.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Filters\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
