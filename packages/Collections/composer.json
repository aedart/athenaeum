{
    "name": "aedart/athenaeum-collections",
    "description": "Collections and Helpers",
    "keywords": [
        "Athenaeum",
        "Collections"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "illuminate/collections": "^8.15",
        "aedart/athenaeum-container": "^5.5",
