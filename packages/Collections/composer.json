{
    "name": "aedart/athenaeum-collections",
    "description": "Collections and data processing utilities",
    "keywords": [
        "Athenaeum",
        "Collections",
        "Items Processor"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "aedart/athenaeum-container": "^8.4",
        "aedart/athenaeum-contracts": "^8.4",
        "aedart/athenaeum-support": "^8.4",
        "aedart/athenaeum-utils": "^8.4",
        "illuminate/collections": "^v11.8.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Collections\\": "src"
        }
    },
    "suggest": {
        "symfony/var-dumper": "symfony/var-dumper: Required to use the dd and dump functions."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
